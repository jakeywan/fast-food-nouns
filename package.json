  "main": "src/index.ts",
  "scripts": {
    "deploy-test": "npx hardhat run --network rinkeby scripts/hardhatDeploy.js",
    "deploy-polygon-nouns-mainnet": "npx hardhat run --network polygon scripts/deployPolyNouns.js",
    "deploy-wearables": "npx hardhat run --network polygon scripts/deployWearables.js",
    "local-test-mint": "npx hardhat run --network localhost scripts/testMint.js",
    "deploy-local": "npx hardhat run --network localhost scripts/deployArbisNouns.js",
    "verify-test": "npx hardhat verify --network rinkeby 0xDB13743795C0D90306085f714b63A0A7A9bf8fC3",
    "exec-polygon": "node scripts/execPolygon.js",
    "deploy-goerli-nouns": "npx hardhat run --network goerli scripts/deployGoerliNouns.js",
    "verify-staker": "npx hardhat verify --network mainnet 0xDA14c912078eBFC6c6d9a1362065a25B7bd81EbF 0x86e4dc95c7fbdbf52e33d563bbdb00823894c287 0xfe5e5D361b2ad62c541bAb87C45a0B9B018389a2",
    "verify-polygon": "npx hardhat verify --network polygon 0x9BE3E6773ffeB13FD8B9176Bb3D479cb75313aF0",
    "deploy-staker": "npx hardhat run --network mainnet scripts/deployStaker.js"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/contracts-upgradeable": "^4.1.0",
