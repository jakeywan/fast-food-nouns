    "base64-sol": "^1.1.0",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.0.32",
    "hardhat": "^2.6.0",
    "prettier-plugin-solidity": "^1.0.0-beta.12"
  },
  "dependencies": {
    "dotenv": "^10.0.0"
  }
}
